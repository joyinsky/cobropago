{
  "name": "cobropago",
  "version": "0.1.0",
  "description": "A powerful expense and income manager",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "mocha frontend/test --compilers js:babel-core/register --require frontend/test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "build-production": "webpack --config webpack.prod.config.js --progress --colors",
    "css-compile": "node-sass --include-path node_modules frontend/sass -o cobropago/web/static/bundles",
    "css-watch": "node-sass --include-path node_modules frontend/sass -o cobropago/web/static/bundles --watch",
    "webpack-watch": "webpack --config webpack.local.config.js --watch",
    "browser-sync": "browser-sync start --files \" cobropago/web/static/bundles/*.css, cobropago/web/static/bundles/*.js, cobropago/**/*.py, cobropago/web/templates/*.html\" --proxy 127.0.0.1:8000 --reload-delay=300 --reload-debounce=500",
    "start": "concurrently --kill-others \"python manage.py runserver\" \"npm run browser-sync\" \"npm run webpack-watch\" \"npm run storybook\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joyinsky/cobropago.git"
  },
  "author": "Luis Fernando Barrera",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/joyinsky/cobropago/issues"
  },
  "homepage": "https://github.com/joyinsky/cobropago#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.5.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "browser-sync": "^2.14.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "es6-promise": "^3.2.1",
    "exports": "0.0.1",
    "fetch": "^1.1.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux-rest-resource": "^0.6.0",
    "redux-thunk": "^2.1.0",
    "uikit": "^2.26.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
